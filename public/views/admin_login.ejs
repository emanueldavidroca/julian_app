                    <div>
                        <input type="checkbox">
                        <label for="">Recordar email</label>
                    </div>
                    <button class="btn btn-primary" type="submit">Iniciar sesión</button>
                </form>
                <hr>
                <a href="" class="link my-3">¿Olvidaste tu contraseña?</a>
                <a href="" class="link">Registrate</a>
            </div>
        </div>
    </section>
